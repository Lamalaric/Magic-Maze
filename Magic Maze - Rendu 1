#------------------------- Import -------------------------------
import time


#------------------------- Fonctions ----------------------------
def creationMap(L, l) :

	map = [ [ None for i in range(l) ]

			for j in range(L)
		  ]

	print(map)



def timer(temps) :
	timeLeft = temps

	for i in range(temps) :
		print(temps)
		time.sleep(1)
		temps -= 1
#------------------------- Main Program -------------------------

if __name__ == '__main__':

	print(creationMap(10,15))
